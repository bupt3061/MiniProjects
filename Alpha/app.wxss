/**app.wxss**/
@import "/dist/index.wxss";
@import "/iconfont/iconfont.wxss";

page {
  background-color: #f3f4f6;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}

.primary {
  color: #14d1b5;
}

.primary-bg {
  color: white;
  background-color:  #14d1b5;
}

.input {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  background-color: white;
  width: 92%;
  padding: 32rpx 32rpx;
  margin-bottom: 32rpx; 
  border: 1rpx solid #eee;
  border-radius: 12rpx;
  box-sizing: border-box;
}

.input .title {
  line-height: 46rpx;
  width: 15%;
}

.input .field {
  width: 85%;
}